    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "versionCode": 1,
      "package": "com.emm",
      "googleServicesFile": "./google-services.json"
    },
    "description": "...",
    "githubUrl": "https://github.com/isthaison/Emm"
    
  }
